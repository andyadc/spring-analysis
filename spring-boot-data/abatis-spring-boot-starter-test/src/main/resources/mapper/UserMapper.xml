<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.andyadc.abatis.spring.boot.test.mapper.UserMapper">

    <select id="selectByName" resultType="com.andyadc.abatis.spring.boot.test.User">
        select *
        from user
        where username = #{username}
    </select>

</mapper>
